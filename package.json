    "graphql-schema-typescript": "^1.3.2",
    "husky": "^4.3.0",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.2",
    "postcss": "^8.1.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
