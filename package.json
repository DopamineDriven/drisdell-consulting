  "license": "CC BY-SA 4.0",
  "private": true,
  "scripts": {
    "dev": "next -p 3333",
    "build": "next build",
    "start": "next start -p 4444",
    "analyze": "ANALYZE=true yarn build",
    "postinstall": "npx patch-package",
    "prettier-check": "prettier --config .prettierrc --check .",
    "prettier-write": "prettier --config .prettierrc --write .",
    "client:download": "dotenv cross-var yarn apollo client:download-schema --includes=graphql --endpoint=%WORDPRESS_API_URL%",
    "client:generate": "yarn apollo codegen:generate --localSchemaFile=schema.json --target=typescript --includes=graphql/**/*.ts --tagName=gql --addTypename --globalTypesFile=types/graphql-global-types.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.2.4",
    "@artsy/fresnel": "^1.2.2",
