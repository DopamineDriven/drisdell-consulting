    "postinstall-postinstall": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-markdown": "^4.3.1",
    "react-spring": "^8.0.27",
    "react-syntax-highlighter": "^15.2.1",
    "swr": "^0.3.5",
    "tailwindcss": "^1.9.1",
    "tailwindcss-dark-mode": "^1.1.6",
    "tailwindcss-gradients": "^3.0.0",
    "typography": "^0.16.19",
    "use-dark-mode": "^2.3.1",
    "vercel": "^20.1.2",
    "wp-graphql": "^1.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
